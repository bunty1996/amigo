<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/newbackground"
    android:orientation="vertical"
    tools:context=".Activities.SelectGender.SelectGenderActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/linearlayout2"
        android:layout_weight="1"
        android:background="@android:color/transparent"
        android:orientation="vertical">

        <com.warkiz.widget.IndicatorSeekBar
            android:id="@+id/indicatorSeekBar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="10dp"
            app:isb_only_thumb_draggable="false"
            app:isb_progress="25"
            android:visibility="gone"
            app:isb_seek_smoothly="true"
            app:isb_show_indicator="rounded_rectangle"
            app:isb_show_tick_marks_type="none"
            app:isb_show_tick_texts="true"
            app:isb_thumb_color="@color/white"
            app:isb_thumb_size="5dp"
            app:isb_tick_marks_size="7dp"
            app:isb_tick_texts_size="0sp"
            app:isb_ticks_count="5"
            app:isb_track_background_color="@android:color/transparent"
            app:isb_track_background_size="2dp"
            app:isb_track_progress_color="@color/malebuttoncolor"
            app:isb_track_progress_size="4dp" />

        <FrameLayout
            android:id="@+id/selGender_container"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </LinearLayout>
</LinearLayout>